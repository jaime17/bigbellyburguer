package big.belly.burguer;

/**
 *
 * @author jaime
 */
public class HealthyBurguer extends Hamburger {
    public HealthyBurguer (String meatType,String breadType, double price){
        super(breadType, meatType,price);
         limAdditions = 6;
    }
    
    
    @Override
    public void getTotal(){
        System.out.println("This is a Healthy Burguer");
        super.getTotal();
    }
    
}
